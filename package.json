{
	"name": "canvideo",
	"displayName": "canvideo",
	"description": "Vscode extension for making animated videos.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:canvideo.helloWorld",
		"onCustomEditor:canvideo.canvideo"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "canvideo.helloWorld",
				"title": "Hello World"
			}
		],
		"customEditors": [
			{
				"viewType": "canvideo.canvideo",
				"displayName": "Canvideo",
				"selector": [
					{
						"filenamePattern": "*.canvideo"
					}
				]
			}
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/preset-react": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@types/mocha": "^9.0.0",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/vscode": "^1.63.0",
		"@types/webpack-env": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"@vscode/test-web": "^0.0.15",
		"assert": "^2.0.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-react-require": "^3.1.3",
		"eslint": "^7.12.1",
		"eslint-config-standard-jsx": "^10.0.0",
		"eslint-config-standard-with-typescript": "^21.0.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.0.0",
		"eslint-plugin-react": "^7.28.0",
		"extract-loader": "^5.1.0",
		"html-loader": "^3.0.1",
		"memfs": "^3.4.0",
		"mocha": "^9.1.3",
		"process": "^0.11.10",
		"ts-loader": "^9.2.5",
		"typescript": "^4",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "^0.8.4",
		"mobx": "^6.3.10",
		"mobx-react-lite": "^3.2.2",
		"path": "^0.12.7",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"vm-browserify": "^1.1.2"
	}
}